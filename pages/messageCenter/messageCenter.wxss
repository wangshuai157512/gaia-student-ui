/* 我的-我的预约 */

.swiper-tab {
  width: 100%;
  height: 70rpx;
  display: -webkit-flex;
  display: flex;
  align-items: center;
  background: #fff;
}

.swiper-tab .swiper-tab-item {
  position: relative;
  width: 50%;
  height: 50rpx;
  text-align: center;
  line-height: 40rpx;
  font-size: 30rpx;
  margin: 0 20rpx;
}

.swiper-tab .active {
  color: #1677FF;

}

.swiper-tab .active::after {
  display: block;
  content: '';
  width: 36%;
  height: 6rpx;
  background: #1677FF;
  position: absolute;
  bottom: 0;
  left: 32.5%;
}

.content {
  width: 100%;
}

.content .list-item-body {
  width: 90%;
  margin: 5% 2.5% 0 2.5%;
  padding: 20rpx;
  font-size: 28rpx;
}


.l-i-b-time {
  display: flex;
  justify-content: center;
  align-items: center;
  margin-bottom: 20rpx;
}

.l-i-b-url {
  display: flex;
  justify-content: center;
  align-items: center;
  background-color: #FFFFFF;
  padding: 0;

}

.l-i-b-url image {
  height: 360rpx;
  /* background-color: #dadada; */
  border: 20rpx solid #ffffff;

}

.l-i-b-title {
  font-size: 32rpx;
  background-color: #FFFFFF;
  padding: 20rpx 20rpx 0 20rpx;
  overflow: hidden;
  text-overflow: ellipsis;
}

.l-i-b-body {
  font-size: 28rpx;
  background-color: #FFFFFF;
  padding: 20rpx;
  color: rgb(90, 90, 90);
  overflow: hidden;
  text-overflow: ellipsis;
}

.nodata {
  display: flex;
  justify-content: center;
  flex-wrap: wrap;
  margin-top: 150rpx;
}

.nodata image {
  width: 50%;
}

.nodata view {
  width: 100%;
  text-align: center;
}

.nodata .imm-res-btn {
  margin-top: 10rpx;
  font-size: 30rpx;
  color: #1677FF
}